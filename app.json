{
  "name": "TNCxCookies",
  "description": "A Telegram bot that generates YouTube cookies dynamically.",
  "repository": "https://github.com/sexyxcoders/COOKIES-GAVER",
  "logo": "https://raw.githubusercontent.com/YourUsername/YouTubeCookiesBot/main/logo.png",
  "keywords": ["telegram", "bot", "youtube", "cookies", "pyrogram"],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "API_ID": {
      "description": "Your Telegram API ID",
      "value": "24054102"
    },
    "API_HASH": {
      "description": "Your Telegram API Hash",
      "value": "564d8467f51c9c28c082175492e17f9a"
    },
    "BOT_TOKEN": {
      "description": "Your Telegram Bot Token",
      "value": "your_bot_token"
    },
    "TIMEZONE": {
      "description": "Default timezone",
      "value": "Asia/Kolkata"
    },
    "DEBUG": {
      "description": "Enable debug mode",
      "value": "True"
    },
    "REQUIRED_CHANNEL": {
      "description": "Users must join this channel",
      "value": "TNCnetwork"
    },
    "LOG_CHANNEL": {
      "description": "Channel ID for bot logs",
      "value": "-1003065367480"
    }
  },
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "standard-1X"
    }
  }
}
